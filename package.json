{
  "name": "boomstrap-react",
  "description": "React Components and Mixins for the Boomstrap library",
  "version": "0.6.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/BoomTownROI/boomstrap-react"
  },
  "main": "dist/index.js",
  "files": [
    "README.md",
    "dist/boomstrap-react.js",
    "dist/**"
  ],
  "peerDependencies": {
    "react": "^0.14.2",
    "react-bootstrap": "^0.27.0",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel": "^5.5.4",
    "babel-core": "^5.5.4",
    "babel-eslint": "^4.0.10",
    "babel-jest": "^4.0.0",
    "babel-loader": "^5.1.3",
    "boomsvgloader": "0.0.2",
    "changelog": "^1.0.7",
    "component-playground": "1.0.3",
    "css-loader": "^0.12.0",
    "eslint": "^1.2.1",
    "eslint-plugin-react": "^3.2.3",
    "gulp": "^3.8.10",
    "gulp-connect": "^2.2.0",
    "gulp-gh-pages": "^0.4.0",
    "gulp-util": "^3.0.4",
    "jest-cli": "^0.2.1",
    "less": "^3.0.0",
    "less-loader": "^2.2.0",
    "marked": "^0.3.2",
    "moment": "^2.8.4",
    "node-libs-browser": "^0.5.2",
    "raw-loader": "^0.5.1",
    "react-docgen": "^1.2.0",
    "react-router": "^0.13.5",
    "react": "^0.14.2",
    "react-bootstrap": "^0.27.0",
    "react-dom": "^0.14.2",
    "style-loader": "^0.12.2",
    "superagent": "^1.2.0",
    "uglifyify": "^3.0.1",
    "vinyl-source-stream": "^1.0.0",
    "webpack": "^1.8.11",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "classnames": "^2.2.0",
    "lodash": "^2.4.1",
    "react-addons-pure-render-mixin": "^0.14.2"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint --ext .js --ext .jsx .",
    "build": "babel src --out-dir dist && gulp",
    "docs": "npm run build && react-docgen dist/Components/ --pretty --o ./docs/docs.json",
    "start": "npm run docs && gulp server",
    "website": "npm run docs && gulp website",
    "prepublish": "npm run build",
    "postpublish": "npm run website",
    "changelog": "changelog boomstrap-react all > CHANGELOG.md"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ]
  }
}
